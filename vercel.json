{
  "version": 2,
  "name": "sprinkler-business-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
    "VITE_GOOGLE_MAPS_API_KEY": "@vite_google_maps_api_key"
  },
  "build": {
    "env": {
      "VITE_SUPABASE_URL": "@vite_supabase_url",
      "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
      "VITE_GOOGLE_MAPS_API_KEY": "@vite_google_maps_api_key"
    }
  },
  "functions": {
    "app/api/**": {
      "maxDuration": 30
    }
  }
} 